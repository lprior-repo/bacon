{
  "name": "bacon",
  "projectType": "application",
  "sourceRoot": "src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mage build",
        "cwd": "{projectRoot}"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mage test",
        "cwd": "{projectRoot}"
      }
    },
    "test-unit": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mage testunit",
        "cwd": "{projectRoot}"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mage lint",
        "cwd": "{projectRoot}"
      }
    },
    "mod-tidy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mage modtidy",
        "cwd": "{projectRoot}"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mage clean",
        "cwd": "{projectRoot}"
      }
    },
    "ci": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mage ci",
        "cwd": "{projectRoot}"
      }
    }
  },
  "tags": ["go", "serverless", "aws", "lambda"]
}